@model List<jenk_test.Models.companyInfo>

<h2>Company List</h2>
<h3>Master Branch</h3>
<p>
    @Html.ActionLink("Create New Company", "Create")
</p>
<table class="table">
    <tr>
        <th>ID</th>
        <th>Company Name</th>
        <th>Employee Count</th>
        <th>Actions</th>
    </tr>
    @foreach (var company in Model)
    {
        <tr>
            <td>@company.id</td>
            <td>@company.companyName</td>
            <td>@company.employeeCount</td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = company.id }) |
                @Html.ActionLink("Edit", "Edit", new { id = company.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = company.id })
            </td>
        </tr>
    }
</table>
